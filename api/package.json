{
  "name": "amos-api",
  "version": "0.1.0",
  "description": "Amos API",
  "main": "src/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/solviofoundation/amos-back/tree/master/api"
  },
  "scripts": {
    "start:prod": "cross-env NODE_ENV=production node -r dotenv-flow/config build/index.js",
    "start": "cross-env NODE_ENV=development npx nodemon -e js,gql,json,cypher --exec npx babel-node -r node_modules/dotenv-flow/config src/index.js",
    "build": "rm -rf build && npx babel src -d build --copy-files",
    "test": "npx jest"
  },
  "dependencies": {
    "apollo-server": "^2.9.3",
    "bcrypt": "^3.0.6",
    "dotenv": "^8.1.0",
    "dotenv-flow": "^3.1.0",
    "got": "^9.6.0",
    "graphql-auth-directives": "^2.0.0",
    "graphql-custom-directives": "^0.2.14",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.1",
    "helmet": "^3.21.0",
    "merge-graphql-schemas": "^1.7.0",
    "metascraper": "^5.7.5",
    "metascraper-title": "^5.7.5",
    "neo4j-driver": "^1.7.6",
    "neo4j-graphql-js": "^2.7.2",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-jwt": "^4.0.0",
    "ramda": "^0.26.1",
    "regenerator-runtime": "^0.13.3"
