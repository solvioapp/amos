    "babel-eslint": "^11.0.0-beta.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "cross-env": "^6.0.0",
    "eslint": "^6.3.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2"
  },
  "optionalDependencies": {
